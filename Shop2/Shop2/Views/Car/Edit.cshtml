@model Shop2.Models.Car.CarViewModel


<h1>Add</h1>


<div>
    <form method="post" enctype="multipart/form-data">
        <input asp-for="Id" type="hidden" />
        <input asp-for="CreatedAt" type="hidden" />
        <input asp-for="ModifiedAt" type="hidden" />

        <div class="form-group row">
            <label asp-for="Make" class="col-sm-2 col-form-label"></label>
            <div class="col-sm-10">
                <input asp-for="Make" class="form-control" />
            </div>
        </div>

        <div class="form-group row">
            <label asp-for="Model" class="col-sm-2 col-form-label"></label>
            <div class="col-sm-10">
                <input asp-for="Model" class="form-control" />
            </div>
        </div>

        <div class="form-group row">
            <label asp-for="Bodytype" class="col-sm-2 col-form-label"></label>
            <div class="col-sm-10">
                <input asp-for="Bodytype" class="form-control" />
            </div>
        </div>

        <div class="form-group row">
            <label asp-for="Year" class="col-sm-2 col-form-label"></label>
            <div class="col-sm-10">
                <input asp-for="Year" class="form-control" />
            </div>
        </div>

        <div class="form-group row">
            <label asp-for="Power" class="col-sm-2 col-form-label"></label>
            <div class="col-sm-10">
                <input asp-for="Power" class="form-control" />
            </div>
        </div>

        <div class="form-group row">
            <label asp-for="Mileage" class="col-sm-2 col-form-label"></label>
            <div class="col-sm-10">
                <input asp-for="Mileage" class="form-control" />
            </div>
        </div>

        <div class="form-group row">
            <label asp-for="Fuel" class="col-sm-2 col-form-label"></label>
            <div class="col-sm-10">
                <input asp-for="Fuel" class="form-control" />
            </div>
        </div>

        <div class="form-group row">
            <label asp-for="Transmission" class="col-sm-2 col-form-label"></label>
            <div class="col-sm-10">
                <input asp-for="Transmission" class="form-control" />
            </div>
        </div>

        <div class="form-group row">
            <label asp-for="Drivetrain" class="col-sm-2 col-form-label"></label>
            <div class="col-sm-10">
                <input asp-for="Drivetrain" class="form-control" />
            </div>
        </div>

        <div class="form-group row">
            <label asp-for="Price" class="col-sm-2 col-form-label"></label>
            <div class="col-sm-10">
                <input asp-for="Price" class="form-control" />
            </div>
        </div>



        <input asp-action="add" asp-controller="car" type="submit" class="btn btn-primary" value="Add">
        <a asp-action="index" class="btn btn-primary">Index</a>

    </form>
</div> 